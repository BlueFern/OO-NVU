title = {{How do astrocytes shape synaptic transmission? Insights from electrophysiology.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3787198{\&}tool=pmcentrez{\&}rendertype=abstract http://journal.frontiersin.org/article/10.3389/fncel.2013.00159/abstract},
volume = {7},
year = {2013}
