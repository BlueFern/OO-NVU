@article{Golubitsky2000,
abstract = {Solutions of reaction-diffusion equations on a circular domain are$\backslash$nconsidered. With Robin boundary conditions, the primary instability$\backslash$nmay be a Hopf bifurcation with eigenfunctions exhibiting prominent$\backslash$nspiral features. These eigenfunctions, defined by Bessel functions$\backslash$nof complex argument, peak near the boundary and are called wall modes.$\backslash$nIn contrast, if the boundary conditions are Neumann or Dirichlet,$\backslash$nthen the eigenfunctions are defined by Bessel functions of real argument,$\backslash$nand take the form of body modes filling the interior of the domain.$\backslash$nBody modes typically do not exhibit pronounced spiral structure.$\backslash$nWe argue that the wall modes are important for understanding the$\backslash$nformation process of spirals, even in extended systems. Specifically,$\backslash$nwe conjecture that wall modes describe the core of the spiral; the$\backslash$nconstant-amplitude spiral visible outside the core is the result$\backslash$nof strong nonlinearities which enter almost immediately above threshold$\backslash$nas a consequence of the exponential radial growth of the wall modes.},
author = {Golubitsky, M and Knobloch, E and Stewart, I},
doi = {10.1007/s003329910013},
file = {:home/agk29/Documents/MendeleyDesktop/Golubitsky, Knobloch, Stewart - 2000 - Target Patterns and Spirals in Planar Reaction-Diffusion Systems.pdf:pdf},
issn = {0938-8974},
journal = {J. Nonlin. Sci.},
keywords = {read},
