journal = {The Journal of neuroscience : the official journal of the Society for Neuroscience},
keywords = {4,5-Trisphosphate Receptors,5-Trisphosphate Receptors: genetics,5-Trisphosphate Receptors: metabolism,Animals,Astrocytes,Astrocytes: metabolism,Calcium Signaling,Cerebrovascular Circulation,Inbred C57BL,Inositol 1,Mice,Visual Cortex,Visual Cortex: blood supply,Visual Cortex: cytology,Visual Cortex: metabolism,Visual Cortex: physiology},
