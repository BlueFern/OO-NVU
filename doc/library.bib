file = {:home/agk29/Documents/MendeleyDesktop/Liu et al. - 2013 - Calcium channels in rat horizontal cells regulate feedback inhibition of photoreceptors through an unconventional GA.pdf:pdf},
issn = {1469-7793},
journal = {The Journal of physiology},
keywords = {Animals,Calcium Channels,Calcium Channels: physiology,Feedback, Physiological,Female,Hydrogen-Ion Concentration,Male,Membrane Potentials,Photoreceptor Cells,Photoreceptor Cells: physiology,Rats,Rats, Sprague-Dawley,Receptors, GABA,Receptors, GABA: physiology,Retinal Horizontal Cells,Retinal Horizontal Cells: physiology,gamma-Aminobutyric Acid,gamma-Aminobutyric Acid: physiology},
pages = {3309--24},
pmid = {23613534},
title = {{Calcium channels in rat horizontal cells regulate feedback inhibition of photoreceptors through an unconventional GABA- and pH-sensitive mechanism.}},
