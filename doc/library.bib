keywords = {3,4 diaminopyridine,4-aminopyridine,blood-,brain barrier,central nervous system,dendritic cells,experi-,leukemia virus,mental allergic encephalomyelitis,multiple sclerosis,murine,voltage-gated potassium channels},
number = {1},
pages = {111},
title = {{Voltage-gated potassium channels in multiple sclerosis: Overview and new implications for treatment of central nervous system inflammation and degeneration}},
