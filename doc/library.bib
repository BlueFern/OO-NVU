file = {:home/agk29/Documents/MendeleyDesktop/Egawa et al. - 2013 - Cl⁻ homeodynamics in gap junction-coupled astrocytic networks on activation of GABAergic synapses.pdf:pdf},
isbn = {1469-7793 (Electronic)$\backslash$r0022-3751 (Linking)},
issn = {1469-7793},
journal = {The Journal of physiology},
