isbn = {1529-2401 (Electronic)},
issn = {1529-2401},
journal = {The Journal of neuroscience : the official journal of the Society for Neuroscience},
keywords = {a 1 receptors and,adenosine,de,glutamate,hippocampus,hypoxia,its neuroprotective role,might be involved in,nmda receptors,synaptic transmission},
number = {21},
pages = {8564--8571},
pmid = {11606644},
title = {{Activation of synaptic NMDA receptors by action potential-dependent release of transmitter during hypoxia impairs recovery of synaptic transmission on reoxygenation.}},
