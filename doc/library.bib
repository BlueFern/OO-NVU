journal = {Biophysical journal},
keywords = {Animals,Calcium,Calcium Signaling,Calcium: metabolism,Electrophysiological Processes,Endothelium, Vascular,Endothelium, Vascular: cytology,Endothelium, Vascular: metabolism,Inositol 1,4,5-Trisphosphate,Inositol 1,4,5-Trisphosphate: metabolism,Ion Channels,Ion Channels: metabolism,Membrane Potentials,Mesenteric Arteries,Mesenteric Arteries: cytology,Mesenteric Arteries: metabolism,Mesenteric Arteries: physiology,Models, Biological,Nitric Oxide,Nitric Oxide: metabolism,Rats,Time Factors},
number = {2},
