month = {jun},
number = {6},
pages = {H2245--------53},
pmid = {12003834},
title = {{Model of nitric oxide diffusion in an arteriole: impact of hemoglobin-based blood substitutes.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/12003834},
volume = {282},
year = {2002}
