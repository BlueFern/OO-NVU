pages = {1--15},
pmid = {26529478},
publisher = {Elsevier Ltd},
title = {{Mathematical model for shear stress dependent NO and adenine nucleotide production from endothelial cells}},
url = {http://dx.doi.org/10.1016/j.niox.2015.10.004},
volume = {52},
year = {2016}
}
