keywords = {Firing pattern,Glial ion channels,Neuron-glia interaction,Potassium buffer,Potassium regulation,Seizures,Spreading depression},
number = {2},
pages = {349--365},
pmid = {18297383},
title = {{Computer simulations of neuron-glia interactions mediated by ion flux}},
volume = {25},
year = {2008}
}
