pmid = {18922953},
title = {{Astrocytes as the glucose shunt for glutamatergic neurons at high activity: an in silico study.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2681423\&tool=pmcentrez\&rendertype=abstract},
