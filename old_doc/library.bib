journal = {Cardiovascular psychiatry and neurology},
month = jan,
pmid = {20862385},
title = {{Targeting S100B in Cerebral Ischemia and in Alzheimer's Disease.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2939387\&tool=pmcentrez\&rendertype=abstract},
