issn = {0090-6964},
journal = {Annals of biomedical engineering},
keywords = {Animals,Arterioles,Arterioles: ultrastructure,Biomechanics,Cheek,Cheek: blood supply,Cricetinae,Microscopy, Electron,Muscle, Smooth, Vascular,Muscle, Smooth, Vascular: physiology},
