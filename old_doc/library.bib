file = {:home/katharina/Dropbox/1PhD\_UCCanterbury/Papers/neue Paper/neuneu/Filosa2004\_Ca\_waves\_NVC.pdf:pdf;:home/katharina/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Filosa, Bonev, Nelson - 2004 - Calcium dynamics in cortical astrocytes and arterioles during neurovascular coupling(2).pdf:pdf},
