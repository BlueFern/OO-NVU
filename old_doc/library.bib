issn = {8750-7587},
journal = {Journal of Applied Physiology},
keywords = {Alzheimer Disease: physiopathology,Brain: blood supply,Brain: physiopathology,Hypertension: physiopathology,Stroke: physiopathology,Vascular: innervation,Vascular: physiopathology,alzheimer disease,alzheimer disease physiopathology,brain,brain blood supply,brain physiopathology,cerebrovascular circulation,humans,hypertension,hypertension physiopathology,muscle,neurons,smooth,stroke,stroke physiopathology,vascular,vascular innervation,vascular physiopathology,vasomotor system},
