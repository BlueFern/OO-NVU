keywords = {Acceleration,Adaptation, Physiological,Adaptation, Physiological: physiology,Animals,Blood Flow Velocity,Blood Flow Velocity: physiology,Blood Pressure,Blood Pressure: physiology,Carotid Arteries,Carotid Arteries: physiology,Cerebral Arteries,Cerebral Arteries: physiology,Cerebrovascular Circulation,Cerebrovascular Circulation: physiology,Computer Simulation,Gravity, Altered,Humans,Models, Cardiovascular},
