journal = {Neurobiology of disease},
keywords = {Animals,Blood-Brain Barrier,Blood-Brain Barrier: anatomy \& histology,Blood-Brain Barrier: metabolism,Blood-Brain Barrier: pathology,Blood-Brain Barrier: physiopathology,Humans,Intracranial Hemorrhages,Intracranial Hemorrhages: metabolism,Nervous System Diseases,Nervous System Diseases: metabolism,Nervous System Diseases: pathology,Nervous System Diseases: physiopathology},
