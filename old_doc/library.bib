file = {:home/katharina/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Takeda et al. - 2010 - Diabetes-accelerated memory dysfunction via cerebrovascular inflammation and Abeta deposition in an Alzheimer mou.pdf:pdf},
issn = {1091-6490},
journal = {Proceedings of the National Academy of Sciences of the United States of America},
keywords = {Alzheimer Disease,Alzheimer Disease: complications,Alzheimer Disease: physiopathology,Amyloid beta-Peptides,Amyloid beta-Peptides: biosynthesis,Animal Feed,Animals,Cerebrovascular Circulation,Crosses, Genetic,Diabetes Mellitus, Experimental,Diabetes Mellitus, Experimental: physiopathology,Disease Models, Animal,Female,Inflammation,Insulin,Insulin: metabolism,Male,Memory Disorders,Memory Disorders: complications,Memory Disorders: physiopathology,Mice,Mice, Inbred C57BL,Mice, Transgenic},
month = apr,
number = {15},
pages = {7036--41},
pmid = {20231468},
title = {{Diabetes-accelerated memory dysfunction via cerebrovascular inflammation and Abeta deposition in an Alzheimer mouse model with diabetes.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2872449\&tool=pmcentrez\&rendertype=abstract},
volume = {107},
year = {2010}
}
