journal = {PLoS biology},
keywords = {Adult,Animals,Astrocytes,Astrocytes: cytology,Astrocytes: virology,Basic Helix-Loop-Helix Transcription Factors,Basic Helix-Loop-Helix Transcription Factors: gene,Basic Helix-Loop-Helix Transcription Factors: meta,Cell Differentiation,Cells,Cerebral Cortex,Cerebral Cortex: cytology,Cerebral Cortex: embryology,Cultured,Genetic,Genetic: methods,Homeodomain Proteins,Homeodomain Proteins: genetics,Homeodomain Proteins: metabolism,Humans,Inbred C57BL,Mice,Nerve Tissue Proteins,Nerve Tissue Proteins: genetics,Nerve Tissue Proteins: metabolism,Neurogenesis,Neurogenesis: physiology,Neuroglia,Neuroglia: cytology,Neuroglia: physiology,Neuroglia: virology,Neurons,Neurons: cytology,Retroviridae,Retroviridae: genetics,Stem Cells,Stem Cells: cytology,Stem Cells: physiology,Synapses,Synapses: physiology,Transcription Factors,Transcription Factors: genetics,Transcription Factors: metabolism,Transduction,Transgenic},
