issn = {0027-8424},
journal = {Proceedings of the National Academy of Sciences of the United States of America},
keywords = {Animals,Arginase,Arginase: biosynthesis,Arginase: genetics,Arginine,Arginine: metabolism,Arginine: pharmacology,Astrocytes,Astrocytes: drug effects,Astrocytes: metabolism,Biological,Cytokines,Cytokines: pharmacology,Enzyme Stability,Enzyme Stability: drug effects,Extracellular Space,Extracellular Space: metabolism,Humans,Messenger,Messenger: biosynthesis,Messenger: genetics,Mice,Models,Nitric Oxide,Nitric Oxide Synthase,Nitric Oxide Synthase Type II,Nitric Oxide Synthase: genetics,Nitric Oxide Synthase: metabolism,Nitric Oxide: biosynthesis,Protein Biosynthesis,Protein Biosynthesis: drug effects,Protein Kinases,Protein Kinases: metabolism,Protein-Serine-Threonine Kinases,RNA,Rats},
month = apr,
