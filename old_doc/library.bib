issn = {00068993},
journal = {Brain Research},
keywords = {Circadian variation,L-Arginine,Microdialysis,Nitric oxide,Pharmacokinetic-pharmacodynamic modeling,Rat},
pages = {67--75},
pmid = {14519513},
title = {{Pharmacokinetics and pharmacodynamics of L-arginine in rats: A model of stimulated neuronal nitric oxide synthesis}},
volume = {989},
year = {2003}
}
