doi = {10.1161/01.CIR.0000074202.19612.8C},
file = {:home/katharina/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Bolz et al. - 2003 - Nitric oxide-induced decrease in calcium sensitivity of resistance arteries is attributable to activation of the my.pdf:pdf},
