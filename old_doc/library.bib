journal = {Circulation},
keywords = {ADP Ribose Transferases,ADP Ribose Transferases: biosynthesis,ADP Ribose Transferases: genetics,ADP Ribose Transferases: pharmacology,Animals,Arteries,Arteries: drug effects,Arteries: physiology,Botulinum Toxins,Botulinum Toxins: biosynthesis,Botulinum Toxins: genetics,Botulinum Toxins: pharmacology,Calcium,Calcium: pharmacology,Cricetinae,Cyclic GMP,Cyclic GMP: metabolism,Enzyme Inhibitors,Enzyme Inhibitors: pharmacology,Female,Intracellular Signaling Peptides and Proteins,Lysophospholipids,Muscles,Muscles: blood supply,Mutagenesis,Myosin-Light-Chain Phosphatase,Nitric Oxide,Nitric Oxide: pharmacology,Phosphoprotein Phosphatases,Phosphoprotein Phosphatases: antagonists \& inhibit,Phosphoprotein Phosphatases: metabolism,Protein-Serine-Threonine Kinases,Protein-Serine-Threonine Kinases: drug effects,Protein-Serine-Threonine Kinases: metabolism,Signal Transduction,Signal Transduction: drug effects,Signal Transduction: physiology,Site-Directed,Sphingosine,Sphingosine: analogs \& derivatives,Sphingosine: pharmacology,Transfection,Vascular Patency,Vascular Patency: drug effects,Vascular Patency: physiology,Vascular Resistance,Vascular Resistance: drug effects,Vascular Resistance: physiology,Vasoconstriction,Vasoconstriction: drug effects,Vasoconstriction: physiology,Vasoconstrictor Agents,Vasoconstrictor Agents: pharmacology,Vasodilator Agents,Vasodilator Agents: pharmacology,rho-Associated Kinases,rhoA GTP-Binding Protein,rhoA GTP-Binding Protein: drug effects,rhoA GTP-Binding Protein: genetics,rhoA GTP-Binding Protein: metabolism},
