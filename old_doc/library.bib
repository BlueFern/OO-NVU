@article{Gruetter1998a,
abstract = {Understanding the mechanism of brain glucose transport across the blood-brain barrier is of importance to understanding brain energy metabolism. The specific kinetics of glucose transport have been generally described using standard Michaelis-Menten kinetics. These models predict that the steady-state glucose concentration approaches an upper limit in the human brain when the plasma glucose level is well above the Michaelis-Menten constant for half-maximal transport, Kt. In experiments where steady-state plasma glucose content was varied from 4 to 30 mM, the brain glucose level was a linear function of plasma glucose concentration. At plasma concentrations nearing 30 mM, the brain glucose level approached 9 mM, which was significantly higher than predicted from the previously reported Kt of approximately 4 mM (p < 0.05). The high brain glucose concentration measured in the human brain suggests that ablumenal brain glucose may compete with lumenal glucose for transport. We developed a model based on a reversible Michaelis-Menten kinetic formulation of unidirectional transport rates. Fitting this model to brain glucose level as a function of plasma glucose level gave a substantially lower Kt of 0.6 +/- 2.0 mM, which was consistent with the previously reported millimolar Km of GLUT-1 in erythrocyte model systems. Previously reported and reanalyzed quantification provided consistent kinetic parameters. We conclude that cerebral glucose transport is most consistently described when using reversible Michaelis-Menten kinetics.},
author = {Gruetter, R and Ugurbil, K and Seaquist, E R},
file = {:home/katharina/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Gruetter, Ugurbil, Seaquist - 1998 - Steady-state cerebral glucose concentrations and transport in the human brain.pdf:pdf},
issn = {0022-3042},
journal = {Journal of neurochemistry},
keywords = {Adult,Biological,Biological Transport,Biological Transport: physiology,Brain,Brain: anatomy \& histology,Brain: metabolism,Glucose,Glucose: metabolism,Homeostasis,Homeostasis: physiology,Humans,Kinetics,Magnetic Resonance Imaging,Magnetic Resonance Spectroscopy,Middle Aged,Models,Osmolar Concentration},
month = jan,
number = {1},
pages = {397--408},
pmid = {9422387},
title = {{Steady-state cerebral glucose concentrations and transport in the human brain.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/9422387},
volume = {70},
year = {1998}
}
@article{Barreto2011,
abstract = {In the past two decades, over 1000 clinical trials have failed to demonstrate a benefit in treating stroke, with the exception of thrombolytics. Although many targets have been pursued, including antioxidants, calcium channel blockers, glutamate receptor blockers, and neurotrophic factors, often the focus has been on neuronal mechanisms of injury. Broader attention to loss and dysfunction of non-neuronal cell types is now required to increase the chance of success. Of the several glial cell types, this review will focus on astrocytes. Astrocytes are the most abundant cell type in the higher mammalian nervous system, and they play key roles in normal CNS physiology and in central nervous system injury and pathology. In the setting of ischemia astrocytes perform multiple functions, some beneficial and some potentially detrimental, making them excellent candidates as therapeutic targets to improve outcome following stroke and in other central nervous system injuries. The older neurocentric view of the central nervous system has changed radically with the growing understanding of the many essential functions of astrocytes. These include K+ buffering, glutamate clearance, brain antioxidant defense, close metabolic coupling with neurons, and modulation of neuronal excitability. In this review, we will focus on those functions of astrocytes that can both protect and endanger neurons, and discuss how manipulating these functions provides a novel and important strategy to enhance neuronal survival and improve outcome following cerebral ischemia.},
author = {Barreto, George and White, Robin E and Ouyang, Yibing and Xu, Lijun and Giffard, Rona G},
file = {:home/katharina/Dropbox/1PhD\_UCCanterbury/Papers/neue Paper/neuneu/von Vera/Barreto2011.pdf:pdf},
