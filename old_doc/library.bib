file = {:home/katharina/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Liu, Garc\'{\i}a-Carde\~{n}a, Sessa - 1996 - Palmitoylation of endothelial nitric oxide synthase is necessary for optimal stimulated release of.pdf:pdf},
