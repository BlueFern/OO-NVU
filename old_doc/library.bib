issn = {0271-678X},
journal = {Journal of cerebral blood flow and metabolism : official journal of the International Society of Cerebral Blood Flow and Metabolism},
keywords = {Animals,Biological,Glucose,Glucose: chemistry,Glucose: metabolism,Glutamic Acid,Glutamic Acid: physiology,Humans,Magnetic Resonance Spectroscopy,Magnetic Resonance Spectroscopy: methods,Models,Neuroglia,Neuroglia: chemistry,Neuroglia: metabolism,Oxidation-Reduction,Sensitivity and Specificity,gamma-Aminobutyric Acid,gamma-Aminobutyric Acid: physiology},
