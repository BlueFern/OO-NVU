file = {:home/katharina/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Gore, Davis - 1984 - Mechanics of smooth muscle in isolated single microvessels.pdf:pdf;:home/katharina/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Gore, Davis - 1984 - Mechanics of smooth muscle in isolated single microvessels(2).pdf:pdf},
issn = {0090-6964},
journal = {Annals of biomedical engineering},
keywords = {Animals,Arterioles,Arterioles: ultrastructure,Biomechanics,Cheek,Cheek: blood supply,Cricetinae,Electron,Microscopy,Muscle,Smooth,Vascular,Vascular: physiology},
month = jan,
number = {5},
