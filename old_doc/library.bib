keywords = {Animals,Brain,Brain: blood supply,Brain: physiology,Cerebrovascular Circulation,Cerebrovascular Circulation: physiology,Electrophysiology,Hemodynamics,Hemodynamics: physiology,Laser-Doppler Flowmetry,Models,Neurological,Rats,Vasoconstriction,Vasoconstriction: physiology,Vasodilation,Vasodilation: physiology,Vibrissae,Vibrissae: innervation},
month = sep,
number = {3},
pages = {1135--1147},
pmid = {20138217},
publisher = {Elsevier Inc.},
title = {{A dynamic model of neurovascular coupling: implications for blood vessel dilation and constriction.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2891822\&tool=pmcentrez\&rendertype=abstract},
volume = {52},
